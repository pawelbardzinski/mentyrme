<h1>Users</h1>

<table>
	<tr>
		<th>First Name</th>
		<th>Last Name</th>
		<th>Mentor?</th>
		<th>Admin?</th>
		<th>Action</th>
	</tr>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.first_name %></td>
			<td><%= user.last_name %></td>
			<td><%= user.is_mentor? %></td>
			<td><%= user.is_admin? %></td>
			<td></td>
		</tr>
	<% end %>
</table>